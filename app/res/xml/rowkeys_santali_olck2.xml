<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key
                latin:keySpec="&#x1C67;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C63;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C6E;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C68;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C5B;"
                latin:moreKeys="&#x1C74;"
                latin:keyHintLabel="&#x1C74;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C6D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C69;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C64;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C73;"
                latin:moreKeys="&#x1C5A;"
                latin:keyHintLabel="&#x1C5A;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C6F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="["
                latin:moreKeys="{"
                latin:keyHintLabel="{"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="]"
                latin:moreKeys="}"
                latin:keyHintLabel="}"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C7F;"
                latin:moreKeys="&#x1C7E;"
                latin:keyHintLabel="&#x1C7E;"
                latin:keyLabelFlags="fontNormal" />
        </case>
        <default>
            <Key
                latin:keySpec="&#x1C67;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C63;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C6E;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C68;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C74;"
                latin:moreKeys="&#x1C5B;"
                latin:keyHintLabel="&#x1C5B;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C6D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C69;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C64;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C5A;"
                latin:moreKeys="&#x1C73;"
                latin:keyHintLabel="&#x1C73;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C6F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="["
                latin:moreKeys="{"
                latin:keyHintLabel="{"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="]"
                latin:moreKeys="}"
                latin:keyHintLabel="}"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C7E;"
                latin:moreKeys="&#x1C7F;"
                latin:keyHintLabel="&#x1C7F;"
                latin:keyLabelFlags="fontNormal" />
         </default>
    </switch>
</merge>
