<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key
                latin:keySpec="&#x1C72;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C7D;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C6A;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C76;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C75;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C78;"
                latin:moreKeys="&#x1C71;"
                latin:keyHintLabel="&#x1C71;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:moreKeys="&#x1C62;"
                latin:keySpec="&#x1C6C;"
                latin:keyHintLabel="&#x1C6C;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&lt;"
                latin:moreKeys=","
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&gt;"
                latin:moreKeys="&#x1C79;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="\?"
                latin:moreKeys="/"
                latin:keyLabelFlags="fontNormal" />
        </case>
        <default>
            <Key
                latin:keySpec="&#x1C72;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C7D;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C6A;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C76;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C75;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C71;"
                latin:moreKeys="&#x1C78;"
                latin:keyHintLabel="&#x1C78;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C62;"
                latin:moreKeys="&#x1C6C;"
                latin:keyHintLabel="&#x1C6C;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec=","
                latin:moreKeys="&lt;"
                latin:keyHintLabel="&lt;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C79;"
                latin:moreKeys="&gt;"
                latin:keyHintLabel="&gt;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="/"
                latin:moreKeys="\?"
                latin:keyHintLabel="\?"
                latin:keyLabelFlags="fontNormal" />
         </default>
    </switch>
</merge>
