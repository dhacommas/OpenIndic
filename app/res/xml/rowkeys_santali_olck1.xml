<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key
                latin:keySpec="~"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="!"
                latin:additionalMoreKeys="!text/morekeys_exclamation"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="\@"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="\#"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x20b9;"
                latin:additionalMoreKeys="!text/morekeys_currency_generic"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="%"
                latin:additionalMoreKeys="!text/morekeys_symbols_percent"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="^"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&amp;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="*"
                latin:additionalMoreKeys="!text/morekeys_star"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="("
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec=")"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="_"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="+"
                latin:additionalMoreKeys="!text/morekeys_plus"
                latin:moreKeys="&#x00B1;,&#x00D7;,&#x00F7;,&#x221A;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
        </case>
        <default>
            <Key
                latin:keySpec="&#x1C7B;"
                latin:moreKeys="`"
                latin:keyHintLabel="`"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C51;"
                latin:moreKeys="!,1"
                latin:keyHintLabel="!"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C52;"
                latin:moreKeys="\@,2"
                latin:keyHintLabel="\@"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C53;"
                latin:moreKeys="\#,3"
                latin:keyHintLabel="\#"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C54;"
                latin:moreKeys="$,&#x20B9;,4"
                latin:keyHintLabel="&#x20B9;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C55;"
                latin:moreKeys="%,5"
                latin:keyHintLabel="%"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C56;"
                latin:moreKeys="^,6"
                latin:keyHintLabel="^"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C57;"
                latin:moreKeys="&amp;,7"
                latin:keyHintLabel="&amp;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C58;"
                latin:moreKeys="*,8"
                latin:keyHintLabel="*"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C59;"
                latin:moreKeys="(,9"
                latin:keyHintLabel="("
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C50;"
                latin:moreKeys="),0"
                latin:keyHintLabel=")"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C7C;"
                latin:moreKeys="-,_"
                latin:keyHintLabel="-"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="="
                latin:moreKeys="+"
                latin:keyHintLabel="+"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
        </default>
    </switch>
</merge>
