<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key
                latin:keySpec="&#x1C5F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C65;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C6B;"
                latin:moreKeys="&#x1C70;"
                latin:keyHintLabel="&#x1C70;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C5D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C5C;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C77;"
                latin:moreKeys="&#x1C66;"
                latin:keyHintLabel="&#x1C66;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C61;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C60;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C5E;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C7A;"
                latin:moreKeys=";"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&quot;"
                latin:moreKeys="'"
                latin:keyLabelFlags="fontNormal" />
        </case>
        <default>
            <Key
                latin:keySpec="&#x1C5F;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C65;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C70;"
                latin:moreKeys="&#x1C6B;"
                latin:keyHintLabel="&#x1C6B;"
                latin:keyLabelFlags="fontNormal|followKeyLetterRatio" />
            <Key
                latin:keySpec="&#x1C5D;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C5C;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C66;"
                latin:moreKeys="&#x1C77;"
                latin:keyHintLabel="&#x1C77;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C61;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C60;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="&#x1C5E;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec=";"
                latin:moreKeys="&#x1C7A;"
                latin:keyHintLabel="&#x1C7A;"
                latin:keyLabelFlags="fontNormal" />
            <Key
                latin:keySpec="'"
                latin:moreKeys="&quot;"
                latin:keyHintLabel="&quot;"
                latin:keyLabelFlags="fontNormal" />
         </default>
    </switch>
</merge>
